<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <title><%= title %></title>
    <link rel="stylesheet" href="./css/main.css">
    <%- alertMsg? `<script>alert("${alertMsg}")</script>` : '' %>
</head>
<body>
<div id="mainPage">
    <div id="mainBox">
        <h1><%= title %></h1>
        <div id="mainMenu">
            <% data.forEach(item => { %>
                <a href="<%= item.href %>" class="mainButton"><%= item.text %></a>
            <% }); %>
        </div>
    </div>
</div>
</body>
</html>
